{
   "background": {
      "persistent": true,
      "scripts": [ "bg.js" ]
   },
   "content_scripts": [ {
      "js": [ "cs.js" ],
      "matches": [ "*://mail.google.com/*" ],
      "run_at": "document_end"
   } ],
   "description": "PixelBlock is a Gmail extension that blocks people from tracking when you open/read their emails.",
   "icons": {
      "128": "images/logo128.png",
      "48": "images/logo48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "PixelBlock",
   "permissions": [ "*://*.googleusercontent.com/proxy/*", "webRequest", "webRequestBlocking" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.0.17",
   "web_accessible_resources": [ "scripts/*.js", "images/*.png", "styles/*.css" ]
}
